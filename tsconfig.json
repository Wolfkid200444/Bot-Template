{
    "compilerOptions": {
        "target": "ES2017",
        "module": "ES2022",
        "strict": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "noEmit": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "typeRoots": ["./src/types", "./node_modules/@types"]
    },
}