{
  "name": "bot-template",
  "version": "1.0.0",
  "description": "bot template",
  "main": "src/bot.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "main": "tsc --project tsconfig.json",
    "dev": "tsx watch src/bot.ts",
    "check": "tsc --noEmit",
    "eslint:check": "eslint ./src",
    "eslint:fix": "eslint ./src --fix",
    "prettier:check": "prettier --check ./src",
    "prettier:fix": "prettier --write ./src",
    "lint": "pnpm run eslint:check && pnpm run prettier:check",
    "lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix"
  },
  "keywords": [],
  "author": "wolfkid200444",
  "license": "MIT",
  "dependencies": {
    "@discordjs/rest": "^1.7.1",
    "discord.js": "^14.11.0",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.2.1",
    "esbuild": "^0.17.19",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8",
    "tsx": "^3.12.7",
    "typescript": "^5.0.4"
  }
}
